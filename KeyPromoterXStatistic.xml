<application>
  <component name="KeyPromoterXStatistic">
    <option name="statistics">
      <Statistic Action="File Structure">
        <Item>
          <IdeaActionID>FileStructurePopup</IdeaActionID>
          <ShortCut>'⌘F12'</ShortCut>
          <Description>File Structure</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="GoTo">
        <Item>
          <IdeaActionID>GotoDeclaration</IdeaActionID>
          <ShortCut>'⌘B' or '⌘Button1 Click' or 'Button2 Click'</ShortCut>
          <Description>GoTo</Description>
          <Count>9</Count>
        </Item>
      </Statistic>
      <Statistic Action="Super Method">
        <Item>
          <IdeaActionID>GotoSuperMethod</IdeaActionID>
          <ShortCut>'⌘U'</ShortCut>
          <Description>Super Method</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Synchronize">
        <Item>
          <IdeaActionID>Synchronize</IdeaActionID>
          <ShortCut>'⌥⌘Y'</ShortCut>
          <Description>Synchronize</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Toggle Line Breakpoint">
        <Item>
          <IdeaActionID>ToggleLineBreakpoint</IdeaActionID>
          <ShortCut>'⌘F8'</ShortCut>
          <Description>Toggle Line Breakpoint</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Type Declaration">
        <Item>
          <IdeaActionID>GotoTypeDeclaration</IdeaActionID>
          <ShortCut>'⇧⌘B' or '⌃⇧B' or '⇧⌘Button1 Click'</ShortCut>
          <Description>Type Declaration</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
    </option>
  </component>
</application>